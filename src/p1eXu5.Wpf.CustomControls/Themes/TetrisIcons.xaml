<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >

    <GeometryGroup x:Key="gm_Close">
        <LineGeometry StartPoint="0 0" EndPoint="10 10" />
        <LineGeometry StartPoint="10 0" EndPoint="0 10" />
    </GeometryGroup>


    <RectangleGeometry x:Key="gm_Maximize" Rect="0.5 0.5 10.5 10.5"></RectangleGeometry>


    <LineGeometry x:Key="gm_Minimize" StartPoint="0.5 10.5" EndPoint="10.5 10.5" />


    <GeometryGroup x:Key="gm_Restore">
        <RectangleGeometry Rect="0.5 3.36 7.14 7.14" />
        <PathGeometry>
            <PathFigure StartPoint="10.5 0.5">
                <PolyLineSegment Points="10.5 0.5 10.5 7.64 7.64 7.64 7.64 3.36 3.36 3.36 3.36 0.5 10.5 0.5"/>
            </PathFigure>
        </PathGeometry>
    </GeometryGroup>

    <PathGeometry x:Key="gm_Next">
        <PathFigure StartPoint="0.5 0.5">
            <PolyLineSegment Points="0.5 0.5 0.5 10.5 8.5 5.75 0.5 0.5"/>
        </PathFigure>
    </PathGeometry>

    <PathGeometry x:Key="gm_Prev" >
        <PathFigure StartPoint="8.5 0.5">
            <PolyLineSegment Points="8.5 0.5 8.5 10.5 0.5 5.75 8.5 0.5"/>
        </PathFigure>
    </PathGeometry>

</ResourceDictionary>